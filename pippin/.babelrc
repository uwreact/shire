{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": "umd"
      }
    ]
  ],
  "plugins": [
    [
      "transform-imports",
      {
        "ramda": {
          "transform": "ramda/src/${member}",
          "preventFullImport": true
        }
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./src"]
      }
    ]
  ]
}
